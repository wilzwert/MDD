{
  "name": "mdd",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "ng e2e",
    "e2e:ci": "ng run mdd:e2e-ci",
    "e2e:coverage": "npx nyc report --report-dir=coverage/e2e --reporter=lcov --reporter=text-summary",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run-record": "cypress run --record --key key"
  },
  "nyc": {
    "report-dir": "coverage/e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^18.4.1",
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.13",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.13",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typeface-inter": "^3.18.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-builders/jest": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.3",
    "@angular/cli": "^18.2.3",
    "@angular/compiler-cli": "^18.2.0",
    "@cypress/code-coverage": "^3.13.7",
    "@cypress/schematic": "^2.5.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "cypress": "^13.16.0",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-instrument": "^6.0.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.3.1",
    "nyc": "^17.1.0",
    "typescript": "~5.5.2",
    "webpack": "^5.96.1"
  }
}
